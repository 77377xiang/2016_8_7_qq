<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.imitationqq">
    <!--网络下载图片的权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.erhu.view.common.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.erhu.view.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.erhu.view.activity.MainActivity" />
        <activity android:name="com.erhu.view.activity.RegisterMainAcivity" />
        <activity android:name="com.erhu.view.activity.ImageActivity" />
        <activity android:name="com.erhu.view.activity.InformationActivity" />
        <activity android:name="com.erhu.view.activity.FirstAcivity" />
        <activity android:name="com.erhu.view.activity.SecondActivity" />
    </application>


</manifest>